dLink href=\"https://xstate.js.org/docs\" target=\"_xstate-docs\">\r\n              Docs\r\n            </StyledLink>\r\n            <StyledLink\r\n              href=\"https://spectrum.chat/statecharts\"\r\n              target=\"_statecharts-community\"\r\n            >\r\n              Community\r\n            </StyledLink>\r\n          </StyledLinks>\r\n        </div>\r\n      </StyledHeader>\r\n    );\r\n  }\r\n}\r\n\r\nclass App extends Component {\r\n  render() {\r\n    return (\r\n      <StyledApp>\r\n        <Header />\r\n        <StateChart machine={myMachine} />\r\n      </StyledApp>\r\n    );\r\n  }\r\n}\r\n\r\nexport default App;\r\n"},"lineCount":null}},"error":null,"hash":"893ec2520912a84610732dea5cdf33bd","cacheData":{"env":{}}}